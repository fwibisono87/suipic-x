# Suipic Environment Variables
# Copy this file to .env and configure for your environment

# PostgreSQL
POSTGRES_USER=suipic
POSTGRES_PASSWORD=suipic_secret_change_me
POSTGRES_DB=suipic

# Keycloak Admin
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin_change_me

# Keycloak Backend Client Secret
KEYCLOAK_BACKEND_SECRET=backend-secret-change-in-production

# Garage S3 (configure after initial setup)
S3_ACCESS_KEY=
S3_SECRET_KEY=
S3_BUCKET=suipic-images

# App URLs (production)
PUBLIC_API_URL=/api
PUBLIC_KEYCLOAK_URL=/auth

# Node environment
NODE_ENV=development
